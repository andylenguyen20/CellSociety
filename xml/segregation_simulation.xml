<?xml version="1.0" encoding="UTF-8" ?>
<simulation>
	<type>Segregation</type>
	<title>Segregation Simulation</title>
	<author>Andy Nguyen</author>
	<param id = "similarityReq">0.5</param>
	<grid>
		<dimension>
			<width>5</width>
			<height>5</height>
		</dimension>
		<cell type = "Segregation">
			<row>0</row>
			<col>1</col>
			<state>2</state>
		</cell>
		<cell type = "Segregation">
			<row>1</row>
			<col>2</col>
			<state>1</state>
		</cell>
		<cell type = "Segregation">
			<row>2</row>
			<col>0</col>
			<state>2</state>
		</cell>
		<cell type = "Segregation">
			<row>2</row>
			<col>1</col>
			<state>1</state>
		</cell>
		<cell type = "Segregation">
			<row>2</row>
			<col>2</col>
			<state>1</state>
		</cell>
		<cell type = "Segregation">
			<row>3</row>
			<col>3</col>
			<state>2</state>
		</cell>
	</grid>
	
</simulation>